<launch>
  <arg name="mav_name" default="reed_quad_x"/>
  <arg name="world_name" default="basic"/>
  <arg name="enable_logging" default="false" />
  <arg name="enable_ground_truth" default="true" />
  <arg name="log_file" default="$(arg mav_name)" />
  <arg name="debug" default="false"/>
  <arg name="gui" default="true"/>
  <arg name="paused" default="true"/>
  <!-- The following line causes gzmsg and gzerr messages to be printed to the console
      (even when Gazebo is started through roslaunch) -->
  <arg name="verbose" default="false"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find rotors_gazebo)/worlds/$(arg world_name).world" />
    <arg name="debug" value="$(arg debug)" />
    <arg name="paused" value="$(arg paused)" />
    <arg name="gui" value="$(arg gui)" />
    <arg name="verbose" value="$(arg verbose)"/>
  </include>

  <group ns="$(arg mav_name)">
    <include file="$(find reed_gazebo)/launch/spawn_mav.launch">
      <arg name="mav_name" value="$(arg mav_name)" />
      <arg name="model" value="$(find reed_gazebo)/urdf/mav_generic_odometry_sensor.gazebo" />
      <param name="robot_description"  command="$(find xacro)/xacro '$(find reed_gazebo)/urdf/reed_quad_x.xacro'" />
      <arg name="enable_logging" value="$(arg enable_logging)" />
      <arg name="enable_ground_truth" value="$(arg enable_ground_truth)" />
      <arg name="log_file" value="$(arg log_file)"/>
    </include>
    <!-- <node name="lee_position_controller_node" pkg="rotors_control" type="lee_position_controller_node" output="screen">
      <rosparam command="load" file="$(find reed_gazebo)/resource/lee_controller_$(arg mav_name).yaml" /> -->
      <node name="mpc_quadrotor_controller" pkg="rotors_control" type="MPC_Quadrotor_Controller_ros.py" output="screen">
      <!-- <node name="mpc_quadrotor_controller" pkg="rotors_control" type="tinympc_wheeltec.py" output="screen"> -->
      <!-- <node name="mpc_quadrotor_controller" pkg="rotors_control" type="incremental_MPC_wheeltec.py" output="screen"> -->
      <!-- <node name="mpc_quadrotor_controller" pkg="rotors_control" type="tinympc_ours.py" output="screen"> -->
      <!-- <node name="mpc_quadrotor_controller" pkg="rotors_control" type="tinympc_ours_copy.py" output="screen"> -->
      <!-- <node name="mpc_controller_node" pkg="rotors_control" type="mpc_controller_node.cpp" output="screen"> -->
      <!-- <rosparam command="load" file="$(find reed_gazebo)/resource/lee_controller_$(arg mav_name).yaml" />  
      <rosparam command="load" file="$(find reed_gazebo)/resource/$(arg mav_name).yaml" /> -->
      <remap from="odometry" to="odometry_sensor1/odometry" />
    </node>
    <node name="hovering_example" pkg="rotors_gazebo" type="hovering_example" output="screen"/>
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
    <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
    <!-- <node name="serial_communicate" pkg="serial_port" type="serial_port.py" /> -->
  </group>
</launch>
